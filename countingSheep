function countSheeps(arrayOfSheep) {
  
    let numSheep = 0
      for(i = 0; i < arrayOfSheep.length; i++){
        if(arrayOfSheep[i] === true){
          numSheep++
        }else if(arrayOfSheep[i] == null || arrayOfSheep[i] == undefined){
          numSheep = numSheep
        }else{
          numSheep = numSheep
        }
      }
      return(numSheep)
      
    }
    
  
